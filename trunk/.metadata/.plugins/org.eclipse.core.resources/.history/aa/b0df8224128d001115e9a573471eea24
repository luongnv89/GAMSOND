package com.gamsond02.object;

public class Individual {
	private int ID;
	private double fitness;
	private WorkingBackup workingChromosome;
	private WorkingBackup backupChromosome;

	public int getID() {
		return ID;
	}

	public void setID(int iD) {
		ID = iD;
	}

	public double getFitness() {
		return fitness;
	}

	public void setFitness(double fitness) {
		this.fitness = fitness;
	}

	public WorkingBackup getWorkingChromosome() {
		return workingChromosome;
	}

	public void setWorkingChromosome(WorkingBackup workingChromosome) {
		this.workingChromosome = workingChromosome;
	}

	public WorkingBackup getBackupChromosome() {
		return backupChromosome;
	}

	public void setBackupChromosome(WorkingBackup backupChromosome) {
		this.backupChromosome = backupChromosome;
	}

	public Individual(int iD, double fitness, WorkingBackup workingChromosome,
			WorkingBackup backupChromosome) {
		super();
		ID = iD;
		this.fitness = fitness;
		this.workingChromosome = workingChromosome;
		this.backupChromosome = backupChromosome;
	}

}
